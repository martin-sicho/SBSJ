<?xml version="1.0" encoding="UTF-8"?>
<form xmlns="http://www.intellij.com/uidesigner/form/" version="1" bind-to-class="views.MainWindow">
  <grid id="27dc6" binding="pContainer" layout-manager="BorderLayout" hgap="0" vgap="0">
    <constraints>
      <xy x="20" y="20" width="526" height="400"/>
    </constraints>
    <properties>
      <preferredSize width="650" height="300"/>
    </properties>
    <border type="none"/>
    <children>
      <grid id="d9ef" binding="pButtons" layout-manager="FlowLayout" hgap="5" vgap="5" flow-align="1">
        <constraints border-constraint="South"/>
        <properties>
          <alignmentX value="0.5"/>
        </properties>
        <border type="none"/>
        <children>
          <component id="d64b8" class="javax.swing.JButton" binding="btDelSelected">
            <constraints/>
            <properties>
              <actionCommand value="btDelSelected"/>
              <text value="Delete Selected"/>
              <toolTipText value="Delete selected backups. Only removes backups from the application. All files are preserved."/>
            </properties>
          </component>
          <component id="429bf" class="javax.swing.JButton" binding="btSyncSele">
            <constraints/>
            <properties>
              <actionCommand value="btSyncSele"/>
              <text value="Synchronize Seleceted"/>
              <toolTipText value="Synchronize selected backups."/>
            </properties>
          </component>
          <component id="2409" class="javax.swing.JButton" binding="btSyncAll">
            <constraints/>
            <properties>
              <actionCommand value="btSyncAll"/>
              <text value="Synchronize All"/>
              <toolTipText value="Synchronize all backups."/>
            </properties>
          </component>
          <component id="2931a" class="javax.swing.JButton" binding="btCreateNew">
            <constraints/>
            <properties>
              <actionCommand value="btCreateNew"/>
              <text value="Create New Backup"/>
              <toolTipText value="Create new backup."/>
            </properties>
          </component>
        </children>
      </grid>
      <scrollpane id="f4fda" binding="scrlPane">
        <constraints border-constraint="Center"/>
        <properties/>
        <border type="none"/>
        <children>
          <component id="8a273" class="javax.swing.JTable" binding="tbTable" default-binding="true">
            <constraints/>
            <properties/>
          </component>
        </children>
      </scrollpane>
      <grid id="385e0" binding="pIntro" layout-manager="BorderLayout" hgap="0" vgap="0">
        <constraints border-constraint="North"/>
        <properties/>
        <border type="empty">
          <size top="5" left="3" bottom="5" right="3"/>
        </border>
        <children>
          <component id="7da23" class="javax.swing.JLabel" binding="lbIntro">
            <constraints border-constraint="Center"/>
            <properties>
              <text value="&lt;html&gt;&#10;&lt;p&gt;&#10;This tiny application provides its user with the ability to make directory or file backups with a simple click of a button.&#10;&lt;/p&gt;&#10;&lt;p&gt;&#10;Below is a table with a summary of all currently scheduled backups. You can manage them directly from here or via the command line interface (use the &lt;code&gt;-h&lt;/code&gt; or &lt;code&gt;--help&lt;/code&gt; option for usage details).&#10;&lt;/p&gt;&#10;&lt;/html&gt;"/>
            </properties>
          </component>
        </children>
      </grid>
    </children>
  </grid>
</form>
